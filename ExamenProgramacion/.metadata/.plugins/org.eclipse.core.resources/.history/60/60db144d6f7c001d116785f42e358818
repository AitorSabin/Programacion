package ejercicio2;

import java.util.Scanner;

public class Matricula {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String c = sc.nextLine();;
		for(int i = 0; i<7; i++) {
			if (i<7) {
				char longitud =c.charAt(i);
				System.out.print(longitud);
			}
		}
		sc.close();
	}
}
