package ejercicio2;

import java.util.Scanner;

public class Matricula {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		String c = sc.nextLine();
		char caracter=' ';
		for(int i = 0;i<=2; i++) {
			caracter = c.charAt(i);
		}
		System.out.println(caracter);
		sc.close();
	}
}
