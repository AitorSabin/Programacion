package ejercicio1;

public class Reloj {

	public static void main(String[] args) {
		int horas = 22;
		int minutos = 0;
		int segundos = 0;
		if (horas <=24) {
			System.out.print(horas + ":");
		}if (minutos <=59) {
			System.out.print(minutos + ":");
		}if(segundos <=59) {}
			System.out.print(segundos);
	}

}
